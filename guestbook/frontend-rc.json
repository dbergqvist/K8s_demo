{
  "apiVersion": "v1beta3",
  "kind": "ReplicationController",
  "metadata": {
    "name": "frontend-v3",
    "labels": {
      "name": "frontend"
    }
  },
  "spec": {
    "replicas": 3,
    "selector": {
      "name": "frontend"
    },
    "template": {
      "metadata": {
        "labels": {
          "name": "frontend"
        }
      },
      "spec": {
        "containers": [{
          "name": "php-guestbook",
          "image": "gcr.io/gcpnext2/php-guestbook:v3",
          "ports": [{
            "containerPort": 80,
            "protocol": "TCP"
          }]
        }]
      }
    }
  }
}
